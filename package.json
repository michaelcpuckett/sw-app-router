{
  "name": "sw-app-router-starter",
  "version": "1.0.0",
  "description": "Serve dynamic HTML with React inside a Service Worker",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --config .prettierrc \"**/*.{ts,tsx}\" --write",
    "dev": "tsx ./.app-router/build/watch.ts",
    "build": "tsx ./.app-router/build/index.ts"
  },
  "app": "public/app.js",
  "client": "public/client.js",
  "targets": {
    "app": {
      "source": "./.app-router/index.tsx",
      "distDir": "./public",
      "context": "service-worker",
      "sourceMap": false
    },
    "client": {
      "source": "./.app-router/client.tsx",
      "distDir": "./public",
      "context": "browser",
      "sourceMap": false
    }
  },
  "author": "Michael Puckett <michael@puckett.contact>",
  "license": "MIT",
  "dependencies": {
    "@express-worker/app": "^1.5.0",
    "lexorank": "^1.0.5",
    "marked": "^15.0.6",
    "pokedex-promise-v2": "^4.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^7.1.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@parcel/transformer-typescript-tsc": "^2.10.3",
    "@types/marked": "^6.0.0",
    "@types/node": "^22.10.7",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/uuid": "^9.0.7",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
    "events": "^3.3.0",
    "http-server": "^14.1.1",
    "parcel": "^2.10.3",
    "prettier": "^3.1.0",
    "process": "^0.11.10",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.2.2"
  }
}
